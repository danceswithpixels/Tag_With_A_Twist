if room == room1 {
    if global.modeNum == 0 {
        if global.score0 == -1 {
            global.score0 = 3600;
            global.score1 = 3600;
            global.score2 = 3600;
            global.score3 = 3600;
        }
        
        global.it = -1;
        
        with(obj_player)
            if sprite_index == spr_it
                global.it = var_player;
                
        if global.it == 0 {
            global.score1--;
            global.score2--;
            global.score3--;
        }
        else if global.it == 1 {
            global.score0--;
            global.score2--;
            global.score3--;
        }
        else if global.it == 2 {
            global.score0--;
            global.score1--;
            global.score3--;
        }
        else if global.it == 3 {
            global.score0--;
            global.score1--;
            global.score2--;
        }
        
        if global.score0 &lt;= 0 or global.score1 &lt;= 0 or global.score2 &lt;= 0 or global.score3 &lt;= 0
            game_restart();
    }
}
