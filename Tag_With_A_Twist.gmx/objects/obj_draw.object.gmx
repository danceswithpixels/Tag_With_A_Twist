            <string>/// Create Players and Blocks

alarm[0]--;

if room == room1 {
    if alarm[0] &lt;= 0 {
        if instance_number(obj_player) == 0
            var_copies = 2;
        else var_copies = 1;
            
        for(j = 0; j &lt; var_copies; j++)
            for(i = 0; i &lt; global.players; i++)
                if instance_number(obj_player) &lt; global.players*3
                    instance_create(random(room_width),random(room_height),obj_player)
        
        if instance_number(obj_block) == 0
            for(i = 0; i &lt; 10; i++)
                instance_create(random(room_width),random(room_height),obj_block)
        
        with(obj_block) {
            x = random(room_width);
            y = random(room_height);
        }
        
        alarm[0] = random_range(300,600);
    }
    if not scr_saveExist()
        with(instance_find(obj_block,floor(random(instance_number(obj_block)))))
            sprite_index = spr_cure;
}
